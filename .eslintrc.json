{
  "extends": ["next/core-web-vitals"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["next/babel"]
    }
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-no-undef": "error",
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
